{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@env": [
        "./env.d.ts"
      ],
      "alchemy-sdk": [
        "node_modules/alchemy-sdk/dist/index.d.ts"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "env.d.ts",
    "expo-env.d.ts"
  ]
}
